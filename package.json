{
  "name": "jacoma_services",
  "version": "0.0.1",
  "description": "Services project Jacoma",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write --print-width=120 --tab-width=4 \"src/**/*.ts\"",
    "start": "ts-node --require tsconfig-paths/register src/index.ts",
    "start:dev": "rimraf dist && npm run build && set debug=* && tsnd --respawn --require tsconfig-paths/register --transpileOnly ./src/index.ts",
    "watch": "tsc-watch --onSuccess \"node ./dist/index.js\" --onFailure \"echo Beep! Compilation Failed\" --compiler typescript/bin/tsc",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node --require ./tsconfig-paths-bootstrap.js dist/index.js",
    "lint": "tslint --fix --force --format verbose \"src/**/*.ts\" \"test/*\"",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run"
  },
  "dependencies": {
    "@types/mailgun-js": "^0.22.3",
    "body-parser": "^1.18.1",
    "class-validator": "^0.9.1",
    "colors": "^1.3.3",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.15.4",
    "express-validator": "^6.1.1",
    "http": "0.0.0",
    "mailgun-js": "^0.22.0",
    "module-alias": "^2.2.0",
    "mysql": "^2.17.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "source-map-support": "^0.5.12",
    "ts-node-dev": "^1.0.0-pre.39",
    "typeorm": "0.2.18"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.0",
    "@types/mysql": "^2.15.6",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "concurrently": "^4.1.0",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsc-watch": "^2.4.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3",
    "wait-on": "^3.2.0"
  },
  "_moduleAliases": {
    "@config": "dist/config",
    "@helpers": "dist/helpers",
    "@routes": "dist/routes",
    "@controllers": "dist/controllers",
    "@entity": "dist/entity",
    "@validation": "dist/routes/validation"
  }
}
